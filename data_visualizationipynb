    {
      "cell_type": "code",
      "source": [
        "iris_dataset = load_iris()\n",
        "data = iris_dataset.data\n",
        "data = iris_dataset.data[:, :2]\n",
        "\n",
        "# Plot data\n",
        "plt.scatter(data[:, 0], data[:, 1], c=\"red\")\n",
        "plt.title(\"Initial Dataset\")\n",
        "plt.xlabel(\"Sepal length\")\n",
        "plt.ylabel(\"Sepal width\")\n",
        "plt.show()"
      ],
